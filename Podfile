target 'Schedule' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Schedule
  pod 'Alamofire', '~> 5.0.0-beta.5'

end

target 'Tests' do
    use_frameworks!
    pod 'Alamofire', '~> 5.0.0-beta.5'
end
